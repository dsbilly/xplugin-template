    "scripts": {
        "test": "xprobe",
        "build": "cross-env NODE_ENV=production NODE_STATS=${stats} webpack --progress --config ./thingjsx.config.js",
        "pack": "cross-env NODE_ENV=production NODE_ARCHIVE=true NODE_STATS=${stats} webpack --progress --config ./thingjsx.config.js",
        "build:prod": "npm run test && npm run build",
        "build:pack": "npm run test && npm run pack",
        "build:prod:stats": "cross-env stats=true npm run build:prod",
        "build:pack:stats": "cross-env stats=true npm run build:pack",
        "mocha": "mocha ./test/*",
        "dev": "cross-env NODE_ENV=development NODE_STATS=true  webpack --progress --config ./thingjsx.config.js",
        "start": "cross-env NODE_ENV=development webpack-dev-server  --progress  --config ./thingjsx.config.js"
    },
